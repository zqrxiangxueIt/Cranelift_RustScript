fn main() -> (r: i64) {
    arr = array [1, 2, 3, 4, 5]
    len = array_len(arr)
    
    sum = 0
    i = 0
    while i < len {
        sum = sum + arr[i]
        i = i + 1
    }
    
    printf("Sum of array elements: %d\n", sum)
    
    array_set(arr, 0, 100)
    
    sum2 = 0
    i = 0
    while i < len {
        sum2 = sum2 + arr[i]
        i = i + 1
    }
    
    printf("New sum after modification: %d\n", sum2)
    
    r = sum2
}
